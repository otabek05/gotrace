    "@mui/material": "^7.0.1",
    "apexcharts": "^4.5.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.34.1",
    "js-cookie": "^3.0.5",
    "minimal-shared": "^1.0.7",
